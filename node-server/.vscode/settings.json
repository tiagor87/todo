// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "src/**/*.d.ts": true,
        "**/*.js.map": true,
        "node_modules": true,
        "typings": true,
        "temp": true
    }
}